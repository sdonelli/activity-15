    "url": "https://github.com/sdonelli/activity-15/issues"
  },
  "homepage": "https://github.com/sdonelli/activity-15#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.14.2",
    "@wdio/local-runner": "^7.6.0",
    "@wdio/mocha-framework": "^7.6.0",
    "@wdio/spec-reporter": "^7.6.0",
    "babel-jest": "^26.6.3",
    "chromedriver": "^90.0.0",
    "jest": "^26.6.3",
    "wdio-chromedriver-service": "^7.1.0"
  },
  "dependencies": {
    "@wdio/cli": "^7.6.0"
