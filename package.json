{
  "name": "activity-15",
  "version": "1.0.0",
  "description": "Writting test for https://www.saucedemo.com/ using WebDriverIO.",
  "main": "index.js",
  "scripts": {
    "test": "jest --color",
    "test-wdio": "npx wdio run ./wdio.conf.js",
    "test-login": "npx wdio run ./wdio.conf.js --spec login.e2e.js",
    "test-footer": "npx wdio run ./wdio.conf.js --spec footer.e2e.js",
    "test-menu": "npx wdio run ./wdio.conf.js --spec menu.e2e.js",
    "test-coverage": "jest --coverage --color"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdonelli/activity-15.git"
  },
